{
  "sd": {
    "access": "${SDAPPLY_ACCESS}",
    "secret": "${SDAPPLY_SECRET}",
    "project": "${SDAPPLY_PROJECT}"
  },
  "fega": {
    "access": "${SDAPPLY_ACCESS}",
    "secret": "${SDAPPLY_SECRET}",
    "project": "${SDAPPLY_PROJECT}",
    "bucket": "${FEGA_BUCKET}",
    "db_string": "${DB_STRING_SDA_FEGA}"
  },
  "bp": {
    "access": "${BIGPICTURE_ACCESS}",
    "secret": "${BIGPICTURE_SECRET}",
    "project": "${BIGPICTURE_PROJECT}",
    "bucket": "${BIGPICTURE_BUCKET}",
    "db_string": "${DB_STRING_SDA_BP}"
  },
  "issuers": [
    {
      "issuer": "${SDAPPLY_ISSUER_NAME}",
      "jku": "${SDAPPLY_ISSUER_JKU}",
      "services": {
        "sd": "",
        "fega": "^EGA*"
      }
    },
    {
      "issuer": "${BIGPICTURE_ISSUER_NAME}",
      "jku": "${BIGPICTURE_ISSUER_JKU}",
      "services": {
        "bp": ""
      }
    }
  ]
}
